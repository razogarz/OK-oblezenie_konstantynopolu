{
  "name": "ok-oblezenie-konstantynopolu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build-tailwind && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-tailwind": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/style.css",
    "build-tailwind-watch": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/style.css --watch",
    "dev-website": "npm install && npm run build-tailwind && npm run dev"
  },
  "dependencies": {
    "daisyui": "^2.51.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.2",
    "yarn": "^1.22.19"
  }
}
